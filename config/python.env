# Python Configuration

# Python command (auto-detected by .config.sh)
# PYTHON_CMD will be set automatically

# Virtual environment
VENV_PROMPT="(neurograph)"

# pip settings
PIP_REQUIRE_VIRTUALENV=true
PIP_DISABLE_PIP_VERSION_CHECK=false

# Testing
PYTEST_WORKERS=auto
PYTEST_VERBOSE=true

# Jupyter
JUPYTER_PORT=8888
JUPYTER_ALLOW_ROOT=false
