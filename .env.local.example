# Example Local Configuration Overrides
# Copy this file to .env.local and customize as needed
# .env.local is ignored by git

# === Project Settings ===

# Environment: development | staging | production
# ENVIRONMENT="development"

# === Ports ===

# Backend API port
# BACKEND_PORT=8000

# Frontend dev server port
# FRONTEND_PORT=5173

# Prometheus metrics port
# PROMETHEUS_PORT=9090

# === Features ===

# Enable/disable Rust Core
# RUST_CORE_ENABLED=true

# Enable/disable Jupyter integration
# JUPYTER_ENABLED=true

# Enable/disable WebSocket support
# WEBSOCKET_ENABLED=true

# Enable/disable Prometheus metrics
# METRICS_ENABLED=true

# === Python ===

# Force specific Python command
# PYTHON_CMD="python3.12"

# Pytest workers (auto | number)
# PYTEST_WORKERS=4

# === Rust ===

# Build profile: debug | release
# RUST_PROFILE="release"

# Rust features
# RUST_FEATURES="python-bindings"

# === Paths ===

# Override project root
# PROJECT_ROOT="/custom/path/to/neurograph"

# Override venv directory name
# VENV_DIR=".venv"

# === Development ===

# Enable verbose logging
# VERBOSE=true

# Enable debug mode
# DEBUG=true
